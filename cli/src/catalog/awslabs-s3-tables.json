{
  "name": "awslabs-s3-tables",
  "title": "AWS S3 Tables",
  "description": "Manage S3 Tables for analytics.",
  "tools": [
    "append_rows_to_table",
    "create_namespace",
    "create_table",
    "create_table_bucket",
    "get_bucket_metadata_config",
    "get_maintenance_job_status",
    "get_table_maintenance_config",
    "get_table_metadata_location",
    "import_csv_to_table",
    "import_parquet_to_table",
    "list_namespaces",
    "list_table_buckets",
    "list_tables",
    "query_database",
    "rename_table",
    "update_table_metadata_location"
  ],
  "env": [
    {
      "name": "AWS_REGION",
      "value": "{{awslabs-s3-tables.aws_region}}"
    },
    {
      "name": "AWS_PROFILE",
      "value": "{{awslabs-s3-tables.aws_profile}}"
    },
    {
      "name": "AWS_ACCESS_KEY_ID",
      "value": "{{awslabs-s3-tables.aws_access_key_id}}"
    }
  ],
  "secrets": [
    {
      "name": "awslabs-s3-tables.aws_secret_access_key",
      "env": "AWS_SECRET_ACCESS_KEY",
      "example": "your-aws-secret-access-key"
    },
    {
      "name": "awslabs-s3-tables.aws_session_token",
      "env": "AWS_SESSION_TOKEN",
      "example": "your-aws-session-token"
    }
  ],
  "config": [
    {
      "name": "awslabs-s3-tables",
      "description": "Configure AWS credentials for S3 Tables access",
      "type": "object",
      "properties": {
        "aws_region": {
          "type": "string"
        },
        "aws_profile": {
          "type": "string"
        },
        "aws_access_key_id": {
          "type": "string"
        }
      }
    }
  ]
}